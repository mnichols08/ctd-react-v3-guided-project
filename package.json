{
  "name": "ctd-react-v3-guided-project",
  "private": true,
  "version": "0.11.2",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:fixture": "cross-env VITE_DATA_SOURCE=fixture vite --host",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:airtable": "cross-env RUN_AIRTABLE_TESTS=1 vitest run --testNamePattern @airtable",
    "test:all:airtable": "cross-env RUN_AIRTABLE_TESTS=1 vitest"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.13.0",
    "styled-components": "^6.3.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.6.2",
    "vite": "^7.1.7",
    "vitest": "^2.1.4"
  }
}
